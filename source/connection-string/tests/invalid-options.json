{
  "tests": [
    {
      "description": "Error on unrecognized option keys",
      "uri": "mongodb://example.com/?foo=bar",
      "valid": false,
      "warning": false,
      "hosts": [
        {
          "type": "hostname",
          "host": "example.com",
          "port": null
        }
      ],
      "auth": null,
      "options": null
    },
    {
      "description": "Error on unsupported option values",
